# Supabase Configuration
# Copy this to .env.local and fill in your values

# Your Supabase project URL
EXPO_PUBLIC_SUPABASE_URL=https://your-project.supabase.co

# Your Supabase anonymous key (safe for client-side)
EXPO_PUBLIC_SUPABASE_ANON_KEY=your-anon-key-here

# App Configuration
EXPO_PUBLIC_APP_NAME=Trophy Angler
EXPO_PUBLIC_APP_URL=https://yourfishingapp.com
EXPO_PUBLIC_API_URL=https://api.yourfishingapp.com

# Optional: Firebase config (if using Firebase for auth)
EXPO_PUBLIC_FIREBASE_API_KEY=
EXPO_PUBLIC_FIREBASE_AUTH_DOMAIN=
EXPO_PUBLIC_FIREBASE_PROJECT_ID=
EXPO_PUBLIC_FIREBASE_STORAGE_BUCKET=
EXPO_PUBLIC_FIREBASE_MESSAGING_SENDER_ID=
EXPO_PUBLIC_FIREBASE_APP_ID=

# Optional: AI Vision API for species identification
EXPO_PUBLIC_GEMINI_API_KEY=

# Mode
NODE_ENV=development
